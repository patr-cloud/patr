{
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "rust-analyzer.cargo.targetDir": "./.devcontainer/volume/data/rust-analyzer",
    "rust-analyzer.procMacro.ignored": {
        "leptos_macro": [
            "component",
            "server"
        ]
    },
    "editor.rulers": [
        100
    ],
    "cSpell.words": [
        "apimachinery",
        "Autoscaler",
        "banzaicloud",
        "BYOC",
        "Citext",
        "DTO",
        "INET",
        "intstr",
        "jsonwebtoken",
        "kubeconfig",
        "leptos",
        "middlewares",
        "onpatr",
        "openapi",
        "opentelemetry",
        "otlp",
        "padawan",
        "proxied",
        "reauthenticated",
        "reauthentication",
        "regcred",
        "sqlx",
        "Stylesheet",
        "TIMESTAMPTZ",
        "tungstenite",
        "wayy",
        "Workspaced"
    ],
    "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix",
    "rust-analyzer.showUnlinkedFileNotification": false,
    "editor.formatOnSave": true,
}