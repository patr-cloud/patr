{
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "rust-analyzer.check.extraArgs": [
        "--target-dir=./target/rust-analyzer"
    ],
    "rust-analyzer.procMacro.ignored": {
        "leptos_macro": [
            "component",
            "server"
        ]
    },
    "editor.rulers": [
        100
    ],
    "cSpell.words": [
        "leptos"
    ],
    "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix"
}